<div class="container">  
  <div class="d-flex justify-content-center">
    <div class="d-flex">
      <h3>Welcome</h3>
    </div>
    <div class="container">
      <div class="d-flex justify-content-center">
        <div class="m-3">
          <div class="mb-3 d-flex justify-content-center">
            <% if !@user.image.blank? %>
              <div class="profile_image">
                <%= image_tag(@user.image, class: "profile_image") %>
              </div>
            <% end %>
          </div>
          <div class="mb-3 d-flex justify-content-center">
            <%= @user.email %>
          </div>
          <div class="mb-3 d-flex justify-content-center">
            <p class="text-black-50"><%= @user.id %>#<%= @user.full_name %></p>
          </div>
          <div class="mt-2 d-flex justify-content-center">
            <%= link_to "Edit", edit_current_user_path, class: "btn btn-dark" %>
          </div>
        </div>
      </div>
    <div>
  </div>
  <div>
    <%= button_tag "test", class: "btn btn-dark text-black hover:text-white", onclick: "test_post_request(this)"%>
  </div>
</div>
<script>
  function test_click(elem){
    console.log(elem)
    $.ajax({
    type: "GET",
    url: "/api/v1/index",
    success: function(data) {
      console.log(data)
    }
  });
  }

  function test_post_request(elem){
    console.log(elem)
    $.post('/api/v1/user', {category:'client', type:'premium'}, function(data){ 
      console.log(data)
    });
  }
</script>